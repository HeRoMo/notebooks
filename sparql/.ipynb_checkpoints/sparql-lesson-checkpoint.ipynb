   "source": [
    "%endpoint http://ja.dbpedia.org/sparql\n",
    "%lang ja\n",
    "%format json\n",
    "%display table\n",
    "\n",
    "PREFIX : <http://ja.dbpedia.org/resource/>\n",
    "PREFIX rdfschema: <http://www.w3.org/2000/01/rdf-schema#>\n",
    "select \n",
    "distinct * \n",
    "where { ?s rdfschema:label \"公園\" .  } \n",
    "LIMIT 100\n"
   ]
